# HedVault Deployment Configuration
# Copy this file to .env and modify the values as needed

# Fee recipient address (receives protocol fees)
FEE_RECIPIENT=0xeeD71459493CDda2d97fBefbd459701e356593f3

# Reward token supply (in wei, 18 decimals)
REWARD_TOKEN_SUPPLY=1000000000000000000000000000000ppp

# Whether to deploy mock ERC20 tokens for testing
DEPLOY_MOCK_TOKENS=true

# Whether to initialize default reward pools
INITIALIZE_REWARD_POOLS=true

# Whether to setup test data and environment
SETUP_TEST_DATA=true

# Private key for deployment (use with caution)
# PRIVATE_KEY=0x0f2d48cef12a3bf037f986780583da871c88d4d7e5e7dcf9bbd6234127703ca3

# RPC URL for the target network
# RPC_URL=https://your-rpc-endpoint

# Etherscan API key for contract verification
# ETHERSCAN_API_KEY=your_etherscan_api_key